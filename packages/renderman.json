{
    "enable": "$RENDERER == 'renderman'",
    "env" : 
    [
        {
            "RMANTREE" :
            [
                {"houdini_os == 'linux'" : "/opt/pixar/RenderManProServer-${RMAN_VER}"},
                {"houdini_os == 'windows'" : "/opt/pixar/RenderManProServer-${RMAN_VER}"}
            ]
        },
        {
            "RFHTREE" : 
            [
                {"houdini_os == 'linux'" : "/opt/pixar/RenderManForHoudini-${RMAN_VER}"},
                {"houdini_os == 'windows'" : "/opt/pixar/RenderManForHoudini-${RMAN_VER}"}
            ]
        },
        {"RMAN_PROCEDURALPATH" : "$RFHTREE/${RFH_VER}/openvdb"},
        {"HOUDINI_PATH" : "$RFHTREE/${RFH_VER}"}
    ]
}